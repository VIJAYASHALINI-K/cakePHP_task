{
    "name": "daoandco/cakephp-cachecleaner",
    "description": "CacheCleaner plugin for CakePHP, this shell clean the cache of your application.",
    "type": "cakephp-plugin",
    "require": {
        "php": ">=5.4.16",
        "cakephp/cakephp": "~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "CacheCleaner\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CacheCleaner\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
